<div class="page-header">
  <h1>
    Programs
    <a href="/programs/new" class="btn btn-lg btn-success">New Program</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Name</th>
        <th>Start date</th>
        <th>End date</th>
        <th>Contact</th>
        <th>Location</th>
        <th>Category</th>
        <th>Actions</th>
      </tr>

      <% @programs.each do |program| %>
      <tr>
        <td><%= program.name %></td>
        <td><%= program.start_date %></td>
        <td><%= program.end_date %></td>
        <td><%= program.contact_id %></td>
        <td><%= program.location %></td>
        <td><%= program.category %></td>
        <td>
          <a href="/programs/<%= program.id %>" class="btn btn-primary">Show</a>
          <a href="/programs/<%= program.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_program/<%= program.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
